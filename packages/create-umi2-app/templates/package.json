{
  "name": "frontend_scaffold",
  "version": "1.1.0",
  "private": true,
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "debug": "node --inspect-brk=8080 node_modules/.bin/umi dev ",
    "analyze": "cross-env ANALYZE=1 umi build",
    "start": "cross-env UMI_UI=none umi dev",
    "start:auth": "cross-env AUTH=1 umi dev",
    "build": "umi build",
    "build:ali": "cross-env AUTH=1 API_HOST=http://47.96.126.125:8068 umi build",
    "test": "umi test",
    "lint": "eslint --ext .js,.jsx src mock -f checkstyle >eslint.xml",
    "precommit": "lint-staged",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "start:no-mock": "cross-env MOCK=none umi dev"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.{js,ts,tsx}": "npm run lint-staged:js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/colors": "^3.1.0",
    "@ant-design/dark-theme": "^2.0.2",
    "@ant-design/pro-layout": "^4.5.16",
    "@antv/data-set": "^0.10.2",
    "@antv/g6": "^3.7.1",
    "ahooks": "^2.6.0",
    "antd": "^4.0.0",
    "classnames": "^2.2.6",
    "dva": "^2.4.1",
    "echarts": "^4.1.0",
    "lodash": "^4.17.11",
    "lodash-decorators": "^6.0.1",
    "moment": "^2.24.0",
    "omit.js": "^1.0.2",
    "path-to-regexp": "^3.1.0",
    "qs": "^6.9.0",
    "rc-queue-anim": "^1.4.0",
    "rc-scroll-anim": "^2.0.2",
    "rc-texty": "^0.2.0",
    "rc-tween-one": "^1.5.5",
    "react": "^16.8.6",
    "react-copy-to-clipboard": "^5.0.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.6",
    "redux": "^4.0.1",
    "slash2": "^2.0.0",
    "crypto-js": "^4.0.0",
    "umi": "^2.9.6",
    "umi-plugin-pro-block": "^1.3.4",
    "umi-plugin-react": "^1.10.1",
    "umi-request": "^1.2.7"
  },
  "devDependencies": {
    "check-prettier": "^1.0.3",
    "cross-env": "^6.0.0",
    "cross-port-killer": "^1.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "enzyme": "^3.9.0",
    "eslint": "5.16.0",
    "eslint-plugin-react-hooks": "^4.1.0",
    "husky": "^4.0.7",
    "jest-puppeteer": "^4.2.0",
    "lint-staged": "^9.0.0",
    "prettier": "^1.17.1",
    "stylelint": "^11.0.0"
  },
  "optionalDependencies": {
    "puppeteer": "^1.17.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
